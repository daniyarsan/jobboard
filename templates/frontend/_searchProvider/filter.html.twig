<div class="accordion">
    <div class="panel-group" id="filter-accordion">
        {% for filter in filterFields %}
            {% if request.get(filter.field.fieldId) is null or request.get(filter.field.fieldId) == '' %}
                <div class="card panel-faq">
                    <div class="card-header">
                        <button class="collapsed" data-toggle="collapse" data-target="#{{ filter.field.fieldId }}" aria-expanded="false" aria-controls="{{ filter.field.fieldId }}">{{ filter.field.name }}</button>
                    </div><!-- card-header -->
                    <div id="{{ filter.field.fieldId }}" class="collapse" data-parent="#filter-accordion">
                        <!-- panel-body -->
                        <div class="panel-body">
                            <ul>
                                {% for opt in filter.options | slice(0, 10) %}
                                    {% set link = request.query.all | merge({(filter.field.fieldId) : opt.title}) %}
                                    <li><a href="{{ path('frontend_jobs_index', link ) }}"> {{ opt.title|truncate(25) }} <span>({{ opt.count }})</span></a></li>
                                {% endfor %}
                            </ul>

                            <ul>
                                {% for key, opt in filter.options %}
                                    {% if key > 10 %}
                                        {% set link = request.query.all | merge({(filter.field.fieldId) : opt.title}) %}
                                        <li><a href="{{ path('frontend_jobs_index', link ) }}"> {{ opt.title|truncate(25) }} <span>({{ opt.count }})</span></a></li>
                                    {% endif %}
                                {% endfor %}
                            </ul>
                        </div><!-- panel-body -->
                    </div>
                </div><!-- card -->
            {% endif %}
        {% endfor %}

